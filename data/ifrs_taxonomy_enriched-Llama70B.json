    "95ec8feb-a360-46e7-8216-89f59221721d": {
        "id": 19,
        "prefLabel": "CLIMATE-RELATED PHYSICAL RISKS",
        "isLeaf": true,
        "definitions": [
            {
                "text": "Risks resulting from climate change that can be event-driven (acute physical risk) or from longer-term shifts in climatic patterns (chronic physical risk). Acute physical risks arise from weather-related events such as storms, floods, drought or heatwaves. Chronic physical risks arise from longer-term shifts in climatic patterns including changes in precipitation and temperature which could lead to sea level rise, reduced water availability, biodiversity loss and changes in soil productivity.",
                "reference": "https://scientificratings.com/2025/07/04/climate-risks-explained-physical-vs-transition/"
            }
        ],
        "tags": [
            "IFRS S2 29 (c)",
            "GRI 201",
            "TCFD",
            "ESRS E1",
            "CDP"
        ],
        "relatedTerms": [
            "acute physical risks",
            "chronic physical risks",
            "extreme weather",
            "floods",
            "storms",
            "hurricanes",
            "droughts",
            "heatwaves",
            "wildfires",
            "sea level rise",
            "temperature changes",
            "precipitation changes",
            "water stress",
            "coastal flooding",
            "asset damage",
            "supply chain disruption",
            "operational disruption"
        ],
        "path_label": [
            "CLIMATE-RELATED RISKS",
            "RISK",
            "ORGANIZATION"
        ],
        "path_id": [
            "5684784f-5fa0-4b0b-9f1a-858feda7acf0",
            "0065aa62-ca77-4d95-971a-42dd948b7721",
            "5d572e35-d7ca-4bbd-bd96-8a49428e3529"
        ],
        "enriched_definition": "Climate-related physical risks refer to event-driven and long-term shifts in climatic patterns, encompassing acute risks from weather events like storms, floods, drought, and heatwaves, as well as chronic risks from changes in precipitation, temperature, sea level rise, water scarcity, biodiversity loss, and soil productivity changes."
    },
